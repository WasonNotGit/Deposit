<?xml version="1.0" encoding="utf-8"?> 
<odoo> 
  <data noupdate="0"> 
    <record id="group_deposit_user" model="res.groups">
        <field name="name">Deposits / user</field>
    </record>
    <record id="deposit_rule" model="ir.rule">
        <field name="name">Just initial rule for group_deposit_user</field>
        <field name="model_id" ref="model_deposit_subject_wason"/>
        <field name="groups" eval="[(4, ref('group_deposit_user'))]"/>
        <field name="perm_read" eval="1"/>
        <field name="perm_write" eval="1"/>
        <field name="perm_create" eval="1"/>
        <field name="perm_unlink" eval="0"/>
        <field name="domain_force">
            [ (1,'=',1) ]
        </field>
    </record>

    <record id="group_deposit_manager" model="res.groups">
        <field name="name">Deposits / Manager</field>
    </record>
    <record id="deposit_rule2" model="ir.rule">
        <field name="name">Just initial rule for group_deposit_manager</field>
        <field name="model_id" ref="model_deposit_subject_wason"/>
        <field name="groups" eval="[(4, ref('group_deposit_manager'))]"/>
        <field name="perm_read" eval="1"/>
        <field name="perm_write" eval="1"/>
        <field name="perm_create" eval="1"/>
        <field name="perm_unlink" eval="1"/>
        <field name="domain_force">
            [ (1,'=',1) ]
        </field>
    </record>
  </data> 
</odoo>